<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title><%= title %></title>
  </head>
  <body>
    <h1><%=title%></h1>
    <ul>
      <% posting.forEach(function(post){ %>
        <li>
          <a href="/posting/<%=post.id %>">
            <%= post.title %>
          </a>
        </li>
      <% }); %>
    </ul>
  </body>
</html>
